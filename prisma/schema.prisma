generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = "file:../data/keys.db"
}

model PublicKey {
  id        Int      @id @default(autoincrement())
  publicKey String
  hash      String   @unique
  createdAt DateTime @default(now())
}
